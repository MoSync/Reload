            if ((userAgent) && (userAgent.length > 0)) {
                var index = 0;
                for (; index < smartPhoneUserAgentRegularExpressions.length; index++) {
                    if (userAgent.match(smartPhoneUserAgentRegularExpressions[index])) {
                        return true;
                    }
                }
            }
            return false;
        }
        /**
         * Returns a <code>Boolean</code> indicating whether or not the specified user agent represents a tablet. If any
